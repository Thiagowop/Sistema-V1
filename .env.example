# Environment Variables Template
# Copy this file to .env and fill in your values

# Application
VITE_APP_NAME=DailyFlow
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_TIMEOUT=180000
VITE_CACHE_VERSION=3.0.0

# ClickUp API (use proxy in production)
# VITE_CLICKUP_API_BASE=/api/clickup-proxy

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=false

# Google OAuth (opcional)
# Para obter: https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com

# ============================================
# SUPABASE (para persistência de configurações)
# ============================================
# Obter em: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Note: Do NOT commit .env file to git!
# Sensitive tokens should be configured in Vercel Dashboard as secrets
